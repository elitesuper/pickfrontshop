{
  "env": {
    "FRAMEWORK_PROVIDER": "graphql",
    "NEXT_PUBLIC_REST_API_ENDPOINT": "http://34.93.17.23",
    "NEXT_PUBLIC_GRAPHQL_API_ENDPOINT": "http://34.93.17.23/graphql",
    "NEXT_PUBLIC_SITE_URL": "",
    "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY": "",
    "NEXT_PUBLIC_PAYPAL_CLIENT_TOKEN": "",
    "NEXT_PUBLIC_RAZORPAY_KEY": "",
    "NEXTAUTH_URL": "http://localhost:3000",
    "SECRET": "",
    "GOOGLE_CLIENT_ID": "",
    "GOOGLE_CLIENT_SECRET": "",
    "NEXT_PUBLIC_ADMIN_URL": "",
    "APPLICATION_MODE": "production",
    "NEXT_PUBLIC_DEFAULT_LANGUAGE": "en",
    "NEXT_PUBLIC_ENABLE_MULTI_LANG": "false",
    "NEXT_PUBLIC_AVAILABLE_LANGUAGES": "en"
  },
  "build": {
    "env": {
      "FRAMEWORK_PROVIDER": "graphql",
      "NEXT_PUBLIC_REST_API_ENDPOINT": "http://34.93.17.23",
      "NEXT_PUBLIC_GRAPHQL_API_ENDPOINT": "http://34.93.17.23/graphql",
      "NEXT_PUBLIC_SITE_URL": "",
      "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY": "",
      "NEXT_PUBLIC_PAYPAL_CLIENT_TOKEN": "",
      "NEXT_PUBLIC_RAZORPAY_KEY": "",
      "NEXTAUTH_URL": "http://localhost:3000",
      "SECRET": "",
      "GOOGLE_CLIENT_ID": "",
      "GOOGLE_CLIENT_SECRET": "",
      "NEXT_PUBLIC_ADMIN_URL": "",
      "APPLICATION_MODE": "production",
      "NEXT_PUBLIC_DEFAULT_LANGUAGE": "en",
      "NEXT_PUBLIC_ENABLE_MULTI_LANG": "false",
      "NEXT_PUBLIC_AVAILABLE_LANGUAGES": "en"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" },
        { "key": "Content-Security-Policy", "value": "upgrade-insecure-requests"}
      ]
    }
  ]
}
